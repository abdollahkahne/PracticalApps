@page
@model PracticalApp.NorthwindEmployee.EmployeeFeature.Pages.EmployeesModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="row">
    <h1 class="display-2">Employees</h1>
</div>
<div class="row">
    @foreach (var item in Model.Employees)
    {
        <div class="col-sm-3">
            <partial name="_Employee" model="item" />
        </div>
    }
</div>
